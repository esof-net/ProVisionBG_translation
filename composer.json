{
  "name": "provision/translation",
  "description": "ProVision Administration translation module",
  "keywords": [
    "provision",
    "module",
    "translation",
    "localization",
    "database"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Venelin Iliev",
      "homepage": "http://veneliniliev.com"
    }
  ],
  "require": {
    "php": ">=8.2.0",
    "stichoza/google-translate-php": "^5.2",
    "provision/administration": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "ProVision\\Translation\\": "src"
    },
    "files": [
      "src/Helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "ProVision\\Translation\\ServiceProvider"
      ],
      "aliases": {
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "url": "../administration",
      "type": "path"
    },
    {
      "url": "../media-manager",
      "type": "path"
    }
  ],
  "config": {
    "cache-dir": "/app/.composer/cache/",
    "allow-plugins": {
      "wikimedia/composer-merge-plugin": true
    }
  }
}
